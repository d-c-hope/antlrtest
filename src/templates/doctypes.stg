doctypes(cfgobject, priIDsMap) ::= <<
    case object £cfgobject.upperName£ extends DocumentType[£cfgobject.name£]("£cfgobject.upperName£",
        £priIDsMap.(cfgobject.name)£,
        Seq(FootballTeamValidator),
